{
  "kind": "collectionType",
  "collectionName": "post_items",
  "info": {
    "name": "Post Items",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Body": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Related_Items": {
      "collection": "post-items"
    },
    "PostOn": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "PostOff": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Media": {
      "type": "component",
      "repeatable": true,
      "component": "common.slider",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Cover": {
      "type": "component",
      "repeatable": true,
      "component": "common.slider",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Router": {
      "model": "router"
    },
    "Catalog": {
      "model": "post-catalogs",
      "via": "Posts"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.common",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Slug": {
      "type": "uid",
      "required": true
    },
    "DisplayOrder": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "Short": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "Rate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "max": 5,
      "min": 0
    },
    "Tags": {
      "via": "Posts",
      "collection": "post-tags"
    },
    "PreviewUrl": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "private": true
    },
    "Author": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "ReadingTime": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "AuthorAvatar": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "common.slider"
    },
    "SubTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    }
  }
}
